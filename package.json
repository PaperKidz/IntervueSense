{
  "name": "interview-platform",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lucide-react": "^0.263.1",
    "react": "^18.x.x",
    "react-dom": "^18.x.x",
    "react-scripts": "^0.0.0",
    "recharts": "^2.x.x"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "backend-start": "cross-env PYTHONUTF8=1 python app.py",
